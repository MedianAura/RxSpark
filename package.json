{
  "name": "RxSpark",
  "productName": "RxSpark",
  "version": "0.0.1",
  "description": "Logiciel pour lancer Perspective ou generer des extrants du moteur.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start -- --debug",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "test": "mocha src/test/unit/**/*.js",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha src/test/unit/**/*.js",
    "integration": "mocha src/test/integration/**/*.js"
  },
  "keywords": [],
  "author": "RxVigilance",
  "license": "GNU",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "x64": [
          "squirrel"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "quiet": true,
        "asar": true,
        "ignore": [
          ".idea",
          "out"
        ]
      }
    }
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "backbone": "^1.3.3",
    "bootstrap": "3.3.7",
    "codemirror": "^5.36.0",
    "configstore": "^3.1.2",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "fs-jetpack": "^1.3.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "nunjucks": "^3.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "electron-forge": "^5.1.2",
    "electron-prebuilt": "^1.4.13",
    "electron-prebuilt-compile": "1.8.4",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "istanbul": "^0.4.5",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.0",
    "proxyquire": "^2.0.1",
    "sinon": "^4.5.0",
    "spectron": "^3.8.0"
  }
}
